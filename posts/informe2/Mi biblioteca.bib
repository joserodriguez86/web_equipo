@book{Poy2019,
	location = {Buenos Aires},
	title = {Mercado de trabajo, políticas sociales y condiciones de vida},
	isbn = {978-987-86-2923-0},
	url = {https://www.teseopress.com/condicionesdevida/},
	abstract = {En la Argentina, los niveles, las tendencias y los factores asociados a los procesos de empobrecimiento y a la desigualdad de las condiciones de vida suscitan amplio interés y despiertan intensos debates en el ámbito académico y en la esfera pública. Un elemento que aviva este interés surge de la constatación de un particular proceso cíclico que, bajo cambiantes escenarios sociopolíticos, parece reproducir niveles de pobreza más elevados que los que se habían logrado alcanzar a mediados de los setenta. Este libro trata de aportar algunas claves sobre el papel que juegan las desigualdades laborales en este proceso.El libro reúne evidencias que sugieren que la heterogeneidad de la estructura ocupacional, la segmentación laboral y la marginalidad económica constituyen instancias estrechamente asociadas con una pauta rígida de desigualdad sociolaboral. Esta pauta de desigualdad se asocia con la pobreza: mientras que una parte de los hogares participa de sectores económicos dinámicos y accede a empleos formales, otra amplia franja permanece ligada a ocupaciones en microempresas, sin protección laboral y con ingresos que no garantizan su subsistencia. La política social –cuya cobertura se extendió a partir de 2003– adquiere creciente relevancia entre los hogares peor posicionados de la estructura ocupacional y los ingresos por transferencias desempeñan un renovado papel en su reproducción económica, aunque resulta limitado en el marco de las condiciones estructurales vigentes.},
	publisher = {Teseo},
	author = {Poy, Santiago},
	urldate = {2020-08-05},
	date = {2019},
	langid = {spanish},
	file = {Snapshot:C\:\\Users\\josed\\Zotero\\storage\\B6P2E243\\condicionesdevida.html:text/html},
}

