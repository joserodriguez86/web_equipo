@article{Barone.etal2022,
	title = {The rise of income and the demise of class and social status? A systematic review of measures of socio-economic position in stratification research},
	volume = {78},
	issn = {0276-5624},
	url = {https://www.sciencedirect.com/science/article/pii/S0276562422000051},
	doi = {10.1016/j.rssm.2022.100678},
	shorttitle = {The rise of income and the demise of class and social status?},
	abstract = {How empirical researchers measure socio-economic position is of great consequence for the understanding of different aspects of the stratification order and affects our conclusions about the causes and consequences of social inequalities. This article surveys social stratification research published in 2015‚Äì2019 in top-cited sociology journals to quantify the prevalence of the three approaches towards social position: income, class and social status. To study trends in researchers' measurement decisions over time, we compare with articles published in the same journals in 1995‚Äì1999. In both periods, income measures dominate stratification research and are gaining ground on class and social status measures over time. However the dominance of income differs between research areas. Intergenerational research on education and social mobility may be the last bastion of occupation-based measures. We also find that {EGP}-like schemes are largely prevalent within class analysis and have gained a paradigmatic status of a "one-size-fits-all" measure. We argue that these trends have profound implications for stratification research in sociology that may herald the death of class-analysis.},
	pages = {100678},
	journaltitle = {Research in Social Stratification and Mobility},
	shortjournal = {Research in Social Stratification and Mobility},
	author = {Barone, Carlo and Hertel, Florian R. and Smallenbroek, Oscar},
	urldate = {2023-10-13},
	date = {2022-04-01},
	keywords = {Social class, Measurement, Income, Position, Socioeconomic, Stratification research, Systematic review, Trend},
	file = {Barone et al. - 2022 - The rise of income and the demise of class and soc.pdf:C\:\\Users\\josed\\Zotero\\storage\\XH6VATTE\\Barone et al. - 2022 - The rise of income and the demise of class and soc.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\josed\\Zotero\\storage\\Z6W3V7JK\\S0276562422000051.html:text/html},
}

@article{Smallenbroek.etal2022,
	title = {Measuring Class Hierarchies in Postindustrial Societies: A Criterion and Construct Validation of {EGP} and {ESEC} Across 31 Countries},
	issn = {0049-1241},
	url = {https://doi.org/10.1177/00491241221134522},
	doi = {10.1177/00491241221134522},
	shorttitle = {Measuring Class Hierarchies in Postindustrial Societies},
	abstract = {In social stratification research, the most frequently used social class schema are based on employment relations ({EGP} and {ESEC}). These schemes have been propelled to paradigms for research on social mobility and educational inequalities and applied in cross-national research for both genders. Using the European Working Conditions Survey, we examine their criterion and construct validity across 31 countries and for both genders. We investigate whether classes are welldelineated by the theoretically assumed dimensions of employment relations and we assess how several measures of occupational advantage differ across classes. We find broad similarity in the criterion validity of {EGP} and {ESEC} across genders and countries as well as satisfactory levels of construct validity. However, the salariat classes are too heterogeneous and their boundaries with the intermediate classes are blurred. To improve the measurement of social class, we propose to differentiate managerial and professional occupations within the lower and higher salariat respectively. We show that implementing these distinctions in {ESEC} and {EGP} improves their criterion validity and allows to better identify privileged positions.},
	pages = {00491241221134522},
	journaltitle = {Sociological Methods \& Research},
	author = {Smallenbroek, O. and Hertel, F. and Barone, C.},
	urldate = {2023-10-13},
	date = {2022-11-11},
	langid = {english},
	note = {Publisher: {SAGE} Publications Inc},
	keywords = {‚úÖ},
	file = {SAGE PDF Full Text:C\:\\Users\\josed\\Zotero\\storage\\G5S7EI8P\\Smallenbroek et al. - 2022 - Measuring Class Hierarchies in Postindustrial Soci.pdf:application/pdf},
