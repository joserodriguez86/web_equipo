@article{Bertaux1969,
	title = {Sur l'analyse des tables de mobilit√© sociale: Le domaine d'√©tude de la mobilit√© sociale},
	volume = {10},
	issn = {0035-2969},
	url = {https://www.jstor.org/stable/3320436},
