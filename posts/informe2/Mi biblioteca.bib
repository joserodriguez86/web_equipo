@article{Bukodi2017,
	title = {Cumulative Inequalities over the Life-Course: Life-long Learning and Social Mobility in Britain},
	volume = {46},
	issn = {0047-2794, 1469-7823},
	url = {https://www.cambridge.org/core/journals/journal-of-social-policy/article/cumulative-inequalities-over-the-lifecourse-lifelong-learning-and-social-mobility-in-britain/261AC63ECE296B875B5AAD632E04355F},
	doi = {10.1017/S0047279416000635},
	shorttitle = {Cumulative Inequalities over the Life-Course},
	abstract = {This paper examines the possibility that life-long learning promotes intergenerational class mobility. The following two research questions are asked. Is it the case that further education provides individuals coming from less advantaged origins with a second chance to improve on their educational attainment? Is it the case that the returns to further qualifications, in terms of chances of upward class career mobility, are greater for children from less advantaged backgrounds than for children from more advantaged backgrounds? The analyses ‚Äì that are based on the complete educational and class histories of men and women in a British birth cohort ‚Äì mainly produce negative findings. Children coming from managerial and professional backgrounds seem to benefit most from further education. More specifically, further education appears to be an effective means of career advancement for individuals of managerial and professional origins who start out in their working lives in relatively low-level class positions. Via further education they can increase or update their qualifications, and in turn enhance their chances of being counter-mobile back to their class of origin. Overall, based on the findings of this paper, we can conclude that qualifications attained through life-long learning primarily serve to maintain, rather than to narrow, inequalities attached to social origins in Britain.},
	pages = {367--404},
	number = {2},
	journaltitle = {Journal of Social Policy},
	author = {Bukodi, Erzs√©bet},
	urldate = {2022-05-06},
	date = {2017-04},
	langid = {english},
	note = {Publisher: Cambridge University Press},
	file = {Bukodi - 2017 - Cumulative Inequalities over the Life-Course Life.pdf:C\:\\Users\\josed\\Zotero\\storage\\Z3WY9RES\\Bukodi - 2017 - Cumulative Inequalities over the Life-Course Life.pdf:application/pdf;Snapshot:C\:\\Users\\josed\\Zotero\\storage\\HNUK4RXT\\261AC63ECE296B875B5AAD632E04355F.html:text/html},
