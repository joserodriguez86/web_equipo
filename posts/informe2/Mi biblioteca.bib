@article{Fernandez-Prados.etal2021,
	title = {Factors explaining social resilience against {COVID}-19: the case of Spain},
	volume = {23},
	issn = {1461-6696},
	url = {https://doi.org/10.1080/14616696.2020.1818113},
	doi = {10.1080/14616696.2020.1818113},
	shorttitle = {Factors explaining social resilience against {COVID}-19},
	abstract = {Between March and May, Spain was one of the hardest-hit European countries by the {COVID}-19 pandemic and registered one of the highest death rates in the world. Among other measures, the political response was a lockdown of more than three months that was applied by means of six fifteen-day extensions. The Spanish Sociological Research Centre (Centro de Investigaciones Sociológicas; {CIS}, in its Spanish acronym) carried out a survey in early June (n = 4,258) asking respondents about their rating of the response to the situation and their ability to cope with further extensions of the state of emergency. The concept of resilience is key to understanding this situation and the population's ability to face up to it. This paper analyses factors that help bolster resilience, which include confidence in the political leader and in the perception or rating of the measures adopted. The conclusion highlights the importance of political communication, both of leadership and of political measures, in fostering social resilience.},
	pages = {S111--S121},
	issue = {sup1},
	journaltitle = {European Societies},
	author = {Fernández-Prados, Juan Sebastián and Lozano-Díaz, Antonia and Muyor-Rodríguez, Jesús},
	urldate = {2021-05-07},
	date = {2021-02-19},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/14616696.2020.1818113},
	keywords = {{COVID}-19, political communication, political trust, Resilience},
	file = {Fernández-Prados et al. - 2021 - Factors explaining social resilience against COVID.pdf:C\:\\Users\\josed\\Zotero\\storage\\4XLEDBLU\\Fernández-Prados et al. - 2021 - Factors explaining social resilience against COVID.pdf:application/pdf;Snapshot:C\:\\Users\\josed\\Zotero\\storage\\64QADYQW\\14616696.2020.html:text/html},
