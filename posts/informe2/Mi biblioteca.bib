	keywords = {‚úÖ, Computer Science - Artificial Intelligence, Computer Science - Computers and Society, Economics - General Economics},
	file = {arXiv Fulltext PDF:C\:\\Users\\josed\\Zotero\\storage\\3G2GV5I3\\Eloundou et al. - 2023 - GPTs are GPTs An Early Look at the Labor Market I.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\josed\\Zotero\\storage\\VVUT3CRR\\2303.html:text/html},
}

