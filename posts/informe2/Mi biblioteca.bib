	langid = {spanish},
	keywords = {Economic inequality, Desigualdad económica, Distributive justice, Estatus social, Inequality perception, Justicia distributiva, Meritocracia, Meritocracy, Percepción de desigualdad, Social status, miradas desigualdad, ✅},
