@article{MarquesPerales.ChavezMolina2019,
	title = {Relevancia de la heterogeneidad socioeconómica. Estudio comparativo entre América Latina y Europa basado en la adaptación del esquema {EGP}},
	volume = {104},
	rights = {Copyright (c) 2018 Ildefonso Marqués Perales, Eduardo Chávez Molina},
	issn = {2013-9004},
	url = {https://papers.uab.cat/article/view/v104-n2-marques-chavez},
	doi = {10.5565/rev/papers.2575},
	abstract = {Los estudios comparativos internacionales de estratificación social y movilidad de clases se basan en esquemas de clases concebidos para los países industrializados. El esquema {EGP}, desarrollado por Erikson, Goldthorpe y Portocarero, se ha convertido en un estándar en el campo de la movilidad social. Sin embargo, este esquema puede no reflejar heterogeneidades en las relaciones laborales que se han agravado en los últimos años producto de la flexibilización, la precarización y la pauperización de los contratos, donde las ocupaciones asalariadas y autónomas están segmentadas en sectores de alta y baja productividad, con condiciones de trabajo y remuneraciones desiguales. La hipótesis que guía nuestro trabajo es la siguiente: el impacto de heterogeneidad en América Latina es tan fuerte que esta dimensión ha de ser considerada en el análisis de clase. Nuestra hipótesis alternativa es que los países del sur de Europa se hallan en una situación intermedia entre América Latina y la Europa tempranamente industrializada. Proponemos una adaptación del esquema {EGP} para explicar tal heterogeneidad. Para ello, utilizamos datos de encuestas nacionales de hogares de algunos países latinoamericanos y también de países del sur de Europa, con la intención de caracterizar sus estructuras de clase y analizar la asociación entre la pertenencia a la clase y las condiciones de vida. La introducción de distinciones en el esquema de clases es relevante en contextos de alta heterogeneidad, no en todos los contextos económicos, por lo cual es esperable que, en sociedades de mayor desarrollo relativo o viceversa, las clases sociales no sean las mismas.},
	pages = {225--245},
	number = {2},
	journaltitle = {Papers. Revista de Sociologia},
	author = {Marqués Perales, Ildefonso and Chávez Molina, Eduardo},
	urldate = {2021-01-12},
	date = {2019},
	langid = {spanish},
	note = {Number: 2},
	keywords = {América Latina, Amèrica Llatina, class scheme, desigualdad, desigualtat, esquema de clases, esquema de classes, Europa, Europe, heterogeneidad socioeconómica, heterogeneïtat socioeconòmica, inequality, Latin America, pobresa, pobreza, poverty, socio-economic heterogeneity},
	file = {Full Text PDF:C\:\\Users\\josed\\Zotero\\storage\\7HYF37MH\\Perales y Molina - 2018 - Relevancia de la heterogeneidad socioeconómica. Es.pdf:application/pdf;Snapshot:C\:\\Users\\josed\\Zotero\\storage\\GS38EWB3\\v104-n2-marques-chavez.html:text/html},
}

