	urldate = {2020-12-11},
	date = {2020-10-01},
	langid = {english},
	keywords = {{COVID}-19, South Korea, gendered division of paid work and household work},
	file = {Lim et al. - 2020 - Men and Women’s Different Dreams on the Future of .pdf:C\:\\Users\\josed\\Zotero\\storage\\XFNBXV69\\Lim et al. - 2020 - Men and Women’s Different Dreams on the Future of .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\josed\\Zotero\\storage\\SRX3PN3N\\S0276562420300809.html:text/html},
